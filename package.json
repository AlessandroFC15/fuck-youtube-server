{
  "name": "fuck-youtube-server",
  "version": "1.0.0",
  "description": "",
  "main": "src",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "start": "babel-node src",
    "lint": "standard",
    "build": "babel src -s -D -d dist",
    "start:prod": "node dist",
    "test": "jest --runInBand --passWithNoTests",
    "test:cover": "jest --runInBand --passWithNoTests --collectCoverage --coverageDirectory ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlessandroFC15/fuck-youtube-server.git"
  },
  "keywords": [],
  "author": "Alessandro Costa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlessandroFC15/fuck-youtube-server/issues"
  },
  "homepage": "https://github.com/AlessandroFC15/fuck-youtube-server#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.16.4",
    "express-validator": "^6.1.1",
    "firebase-admin": "^8.5.0",
    "jest": "^24.8.0",
    "puppeteer": "^1.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "nodemon": "^1.18.9",
    "standard": "^12.0.1",
    "supertest": "^4.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "jest": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
